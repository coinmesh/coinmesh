{
  "name": "trading-api",
  "version": "0.0.1",
  "scripts": {
    "prestart": "npm install",
    "start": "nodemon ./bin/www",
    "test": "nodemon --exec 'jasmine'"
  },
  "dependencies": {
    "@coinmesh/gemini-adapter": "0.0.1",
    "@coinmesh/mongodb-adapter": "0.0.1",
    "express": "^4.16.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "jasmine": "^3.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0"
  },
  "main": "index.js",
  "keywords": [
    "coinmesh",
    "gemini"
  ],
  "homepage": "http://coinmesh.com",
  "license": "CC0-1.0",
  "author": "Patrick Walters <patrick.walters@blockfuse.io> (http://blockfuse.io/)",
  "repository": {
    "type": "git",
    "url": "http://github.com/coinmesh"
  },
  "coinmesh": {
    "type": "logic-service",
    "events": {
      "loading": [
        "/trading-api.*prestart/i"
      ],
      "waiting": [
        ""
      ],
      "ready": [
        "/trading-api.*nodemon.*starting/i"
      ],
      "failed": [
        "/trading-api.*nodemon.*app.*crashed/i"
      ]
    }
  }
}
