{
  "name": "@coinmesh/gemini-adapter",
  "version": "0.0.2",
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "nodemon --exec 'jasmine'"
  },
  "devDependencies": {
    "jasmine": "^3.1.0",
    "nodemon": "^1.17.4"
  },
  "main": "index.js",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "coinmesh": {
    "type": "adapter",
    "events": {
      "loading": [
        "/trading-api.*prestart/i"
      ],
      "waiting": [
        ""
      ],
      "ready": [
        "/trading-api.*nodemon.*starting/i"
      ],
      "failed": [
        "/trading-api.*nodemon.*app.*crashed/i"
      ]
    }
  }
}
