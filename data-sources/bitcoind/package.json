{
  "name": "@coinmesh/bitcoind",
  "version": "0.0.1",
  "description": "Bitcoind Data Source for CoinMesh.",
  "coinmesh": {
    "type": "data-source",
    "adapters": {
      "bitcoind-adapter": "adapters/bitcoind-adapter"
    },
    "network": "regtest",
    "runHttp": true,
    "confFilePath": "bitcoin.conf",
    "conf": {
      "regtest": "1",
      "server": "1",
      "rpcuser": "rpcuser",
      "rpcpassword": "rpcpassword",
      "txindex": "1",
      "zmqpubrawblock": "tcp://127.0.0.1:28332",
      "zmqpubrawtx": "tcp://127.0.0.1:28332"
    },
    "events": {
      "loading": ["/bitcoin.*validating.*signatures.*for.*all.*blocks/i"],
      "ready": ["/init.*message.*done.*loading/i"],
      "failed": ["/shutdown.*done/i"]
    },
    "actions": {
      "purgeBitcoindChainData": "bitcoind"
    }
  },
  "main": "index.js",
  "scripts": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coinmesh/coinmesh.git"
  },
  "keywords": [
    "coinmesh",
    "bitcoind"
  ],
  "author": "Patrick Walters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coinmesh/coinmesh/issues"
  },
  "homepage": "https://coinmesh.com"
}
